# ==============================================================================
# ENVIRONMENT VARIABLES TEMPLATE
# ==============================================================================
# Copy this file to .env and fill in your actual values
# Never commit .env to git (it's in .gitignore)

# ------------------------------------------------------------------------------
# AlphaVantage API
# ------------------------------------------------------------------------------
# Get your free API key at: https://www.alphavantage.co/support/#api-key
# Free tier: 25 requests/day, 5 requests/minute
ALPHA_VANTAGE_API_KEY=your_alphavantage_api_key_here

# ------------------------------------------------------------------------------
# Supabase API Credentials
# ------------------------------------------------------------------------------
# Get from: Supabase Dashboard → Settings → API
# Use the "Project URL" and "anon/public" key (NOT service_role key)
SUPABASE_URL=https://xxxxxxxxxxxxx.supabase.co
SUPABASE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...your_anon_key_here

# ------------------------------------------------------------------------------
# Supabase Database Connection
# ------------------------------------------------------------------------------
# Get from: Supabase Dashboard → Settings → Database → Connection string
# Use Direct connection (not pooler) for simplicity
SUPABASE_DB_HOST=db.xxxxxxxxxxxxx.supabase.co
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=postgres
SUPABASE_DB_PASSWORD=your_database_password_here
SUPABASE_DB_PORT=5432